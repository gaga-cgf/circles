<h1>管理员后台-用户列表</h1>

<div class="row">
  <div class="col-sm-12">
    <form action="<%= search_admin_users_path %>" method="get">
      用户名：<input type="text" name="username" value="<%= params[:username]%>">
      <input type="submit" name="搜索">
    </form>
    <table class="table">
      <tr>
        <th>ID</th>
        <th>用户名</th>
        <th></th>
      </tr>
      <% @users.each do |user| %>
        <%= render partial: 'one_user', collection: @users, as: :user %>
      <% end %>
    </table>

    <%= will_paginate @users%>
  </div>
</div>